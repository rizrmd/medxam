{
  "name": "medxamion",
  "version": "1.0.0",
  "description": "National Orthopaedic and Traumatology Board Examination System",
  "scripts": {
    "dev": "concurrently --kill-others-on-fail \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "cd backend && mise exec -- go run ./cmd/api",
    "dev:frontend": "cd frontend && npm run dev",
    "dev:tmux": "./start-dev-tmux.sh",
    "stop": "./stop-dev.sh",
    "backend": "cd backend && mise exec -- go run ./cmd/api",
    "frontend": "cd frontend && npm run dev",
    "build": "npm run build:frontend && npm run build:backend",
    "build:frontend": "cd frontend && npm run build",
    "build:backend": "cd backend && go build -o ../dist/api ./cmd/api",
    "prod": "concurrently \"npm run prod:backend\" \"npm run prod:frontend\"",
    "prod:backend": "./dist/api",
    "prod:frontend": "cd frontend && npm run preview",
    "install:all": "npm install && npm run install:frontend && npm run install:backend",
    "install:frontend": "cd frontend && npm install",
    "install:backend": "cd backend && mise install",
    "start": "npm run dev",
    "test": "playwright test",
    "test:ui": "playwright test --ui",
    "test:headed": "playwright test --headed",
    "test:debug": "playwright test --debug"
  },
  "devDependencies": {
    "@playwright/test": "^1.54.2",
    "concurrently": "^8.2.2",
    "playwright": "^1.54.2"
  },
  "keywords": [
    "exam",
    "medical",
    "orthopaedic"
  ],
  "author": "",
  "license": "ISC"
}
